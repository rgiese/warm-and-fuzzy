{
  "name": "@grumpycorp/warm-and-fuzzy-shared",
  "description": "WarmAndFuzzy shared types",
  "author": "robin@grumpycorp.com",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgiese/warm-and-fuzzy.git"
  },
  "version": "1.0.0",
  "scripts": {
    "prebuild": "npm-run-all codegen:*",
    "build": "tsc",
    "codegen:graphql": "graphql-codegen --config graphql-codegen.yml",
    "format:fix": "prettier --write {.,./src/**,./public/**}/*.{css,html,js,jsx,json,md,scss,ts,tsx,webmanifest}",
    "lint": "eslint {.,./**}/*.{ts,tsx}",
    "lint:fix": "eslint --fix {.,./**}/*.{ts,tsx}"
  },
  "dependencies": {
    "graphql": "^14.4.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.3.1",
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/typescript": "^1.3.1",
    "@types/node": "12.0.8",
    "@types/yup": "^0.26.21",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "3.5.2"
  }
}
