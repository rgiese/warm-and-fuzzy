{
  "name": "@grumpycorp/warm-and-fuzzy-shared",
  "version": "1.0.0",
  "private": true,
  "description": "WarmAndFuzzy shared types",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgiese/warm-and-fuzzy.git"
  },
  "license": "CC-BY-NC-SA-4.0",
  "author": "robin@grumpycorp.com",
  "files": [
    "build"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "prebuild": "npm-run-all codegen:*",
    "build": "tsc",
    "clean": "rimraf build src/generated",
    "codegen:flatbuffers": "ts-node ./codegen-flatbuffers.ts",
    "codegen:graphql": "graphql-codegen --config graphql-codegen.ts",
    "format:fix": "prettier --write \"{.,./src/**,./public/**}/*.{css,graphql,html,js,jsx,json,md,scss,ts,tsx,webmanifest}\"",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "test": ""
  },
  "dependencies": {
    "flatbuffers": "^23.5.26",
    "graphql": "^16.8.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1"
  }
}
