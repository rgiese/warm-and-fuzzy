{
  "name": "@grumpycorp/warm-and-fuzzy-mobile-reactnative",
  "version": "1.0.0",
  "private": true,
  "description": "WarmAndFuzzy mobile app",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgiese/warm-and-fuzzy.git"
  },
  "license": "CC-BY-NC-SA-4.0",
  "author": "robin@grumpycorp.com",
  "scripts": {
    "format:fix": "prettier --write {.,./src/**}/*.{css,js,jsx,json,md,scss,ts,tsx}",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "start-mobile:local:dev": "react-native run-android",
    "start-mobile:local:prod": "react-native run-android",
    "start-mobile:remote:dev": "react-native run-android",
    "start-mobile:remote:prod": "react-native run-android",
    "bundle-mobile": "run-script-os",
    "bundle-mobile:win32": "jetify && cd .\\android && ./gradlew.bat clean && ./gradlew.bat bundleRelease",
    "bundle-mobile:default": "jetify && cd ./android && ./gradlew clean && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@grumpycorp/warm-and-fuzzy-shared": "^1.0.0",
    "@types/react-native-auth0": "^1.3.0",
    "react": "^16.8.6",
    "react-native": "^0.60.4",
    "react-native-auth0": "^1.5.0"
  }
