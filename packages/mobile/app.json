{
  "expo": {
    "name": "Warm and Fuzzy",
    "description": "A custom home HVAC automation system - https://warmandfuzzy.house",
    "slug": "warm-and-fuzzy",
    "privacy": "unlisted",
    "owner": "rgiese",
    "githubUrl": "https://github.com/rgiese/warm-and-fuzzy",
    "version": "1.0.0",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android"],
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/**/*"],
    "scheme": "warmandfuzzy",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000"
    },
    "android": {
      "package": "house.warmandfuzzy.app",
      "versionCode": 1
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
