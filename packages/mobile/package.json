{
  "name": "@grumpycorp/warm-and-fuzzy-mobile",
  "version": "1.0.0",
  "private": true,
  "description": "WarmAndFuzzy mobile app",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgiese/warm-and-fuzzy.git"
  },
  "license": "CC-BY-NC-SA-4.0",
  "author": "robin@grumpycorp.com",
  "scripts": {
    "build-mobile": "expo build:android",
    "format:fix": "prettier --write {.,./src/**}/*.{css,js,jsx,json,md,scss,ts,tsx}",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "deploy-mobile:dev": "expo publish --release-channel dev --non-interactive",
    "deploy-mobile:prod": "expo publish --release-channel prod --non-interactive",
    "start-mobile:local:dev": "expo start",
    "start-mobile:local:prod": "expo start",
    "start-mobile:remote:dev": "expo start",
    "start-mobile:remote:prod": "expo start"
  },
  "main": "src/App.tsx",
  "dependencies": {
    "@grumpycorp/warm-and-fuzzy-shared": "^1.0.0",
    "@types/nanoid": "^2.0.0",
    "expo": "^34.0.1",
    "expo-random": "^6.0.0",
    "jwt-decode": "^2.2.0",
    "nanoid": "^2.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-web": "^0.11.4"
  }
}
