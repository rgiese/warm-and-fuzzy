{
  "name": "@grumpycorp/warm-and-fuzzy-mobile",
  "version": "1.0.0",
  "private": true,
  "description": "WarmAndFuzzy mobile app",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgiese/warm-and-fuzzy.git"
  },
  "license": "CC-BY-NC-SA-4.0",
  "author": "robin@grumpycorp.com",
  "scripts": {
    "codegen:graphql": "graphql-codegen --config graphql-codegen.yml",
    "format:fix": "prettier --write {.,./src/**}/*.{css,js,jsx,json,md,scss,ts,tsx}",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "decrypt-secrets": "npm-run-all decrypt-secrets:*",
    "decrypt-secrets:keystore": "cross-var openssl aes-256-cbc -d -out android/app/waf-upload-key.keystore.decrypted -in android/app/waf-upload-key.keystore.encrypted -k $WAF_GIT_SECRETS_KEY",
    "decrypt-secrets:keystore-password": "cross-var openssl aes-256-cbc -d -out android/app/waf-upload-key.password.decrypted -in android/app/waf-upload-key.password.encrypted -k $WAF_GIT_SECRETS_KEY",
    "decrypt-secrets:google-play-service-account": "cross-var openssl aes-256-cbc -d -out android/app/google-play-service-account.p12.decrypted -in android/app/google-play-service-account.p12.encrypted -k $WAF_GIT_SECRETS_KEY",
    "encrypt-secrets": "npm-run-all encrypt-secrets:*",
    "encrypt-secrets:keystore": "cross-var openssl aes-256-cbc -e -in android/app/waf-upload-key.keystore.decrypted -out android/app/waf-upload-key.keystore.encrypted -k $WAF_GIT_SECRETS_KEY",
    "encrypt-secrets:keystore-password": "cross-var openssl aes-256-cbc -e -in android/app/waf-upload-key.password.decrypted -out android/app/waf-upload-key.password.encrypted -k $WAF_GIT_SECRETS_KEY",
    "encrypt-secrets:google-play-service-account": "cross-var openssl aes-256-cbc -e -in android/app/google-play-service-account.p12.decrypted -out android/app/google-play-service-account.p12.encrypted -k $WAF_GIT_SECRETS_KEY",
    "prestart-mobile:local:dev": "npm-run-all codegen:*",
    "start-mobile:local:dev": "cross-env ENVFILE=.env.local react-native run-android",
    "prestart-mobile:local:prod": "npm-run-all codegen:*",
    "start-mobile:local:prod": "cross-env ENVFILE=.env.local react-native run-android",
    "prestart-mobile:remote:dev": "npm-run-all codegen:*",
    "start-mobile:remote:dev": "cross-env ENVFILE=.env.dev react-native run-android",
    "prestart-mobile:remote:prod": "npm-run-all codegen:*",
    "start-mobile:remote:prod": "cross-env ENVFILE=.env.prod react-native run-android",
    "prebundle-mobile": "npm-run-all codegen:*",
    "bundle-mobile": "cross-env ENVFILE=.env.prod run-script-os",
    "bundle-mobile:win32": "jetify && cd .\\android && gradlew.bat clean && gradlew.bat bundleRelease",
    "bundle-mobile:default": "jetify && cd ./android && ./gradlew clean && ./gradlew bundleRelease",
    "clean-mobile": "cross-env ENVFILE=.env.prod run-script-os",
    "clean-mobile:win32": "cd .\\android && gradlew.bat clean",
    "clean-mobile:default": "cd ./android && ./gradlew clean",
    "publish-mobile:internal": "cross-env ENVFILE=.env.prod run-script-os",
    "publish-mobile:internal:win32": "node tools/create-android-release-name.js && cd .\\android && gradlew.bat publishReleaseBundle",
    "publish-mobile:internal:default": "node tools/create-android-release-name.js && cd ./android && ./gradlew publishReleaseBundle",
    "android:logcat": "adb -s emulator-5554 logcat -s \"ReactNativeJS\""
  },
  "dependencies": {
    "@grumpycorp/warm-and-fuzzy-shared": "^1.0.0",
    "@grumpycorp/warm-and-fuzzy-shared-client": "^1.0.0",
    "@react-native-community/datetimepicker": "^2.1.0",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/slider": "^2.0.8",
    "@types/clone-deep": "^4.0.1",
    "@types/react-native-auth0": "^2.0.0",
    "@types/react-native-elements": "^0.18.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "clone-deep": "^4.0.1",
    "metro": "^0.58.0",
    "mobx": "^5.15.2",
    "mobx-react": "^6.1.5",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-fast-compare": "^2.0.4",
    "react-native": "^0.61.5",
    "react-native-auth0": "^2.1.1",
    "react-native-config": "^0.12.0",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-keychain": "^4.0.5",
    "react-native-paper": "^3.4.1",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.0.0-alpha.26",
    "react-native-svg": "^11.0.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^2.0.15"
  }
}
