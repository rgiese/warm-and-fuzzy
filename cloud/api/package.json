{
  "name": "warm-and-fuzzy-api",
  "description": "WarmAndFuzzy API",
  "author": "robin@grumpycorp.com",
  "version": "1.0.0",
  "scripts": {
    "prestart": "npm-run-all codegen:*",
    "start": "sls offline start",
    "predeploy:dev": "npm-run-all codegen:*",
    "deploy:dev": "sls deploy -v",
    "predeploy:prod": "npm-run-all codegen:*",
    "deploy:prod": "sls deploy -v --stage prod",
    "codegen:graphql": "graphql-codegen --config graphql-codegen.yml",
    "dev:tsc": "tsc",
    "format:fix": "prettier --write {.,../shared/**,./resources/**,./src/**}/*.{graphql,js,json,ts,yml}",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "apollo-server-lambda": "^2.6.7",
    "bufferutil": "^4.0.1",
    "graphql": "^14.4.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.5.1",
    "serverless-domain-manager": "^3.2.6",
    "serverless-offline": "^5.5.1",
    "source-map-support": "^0.5.10",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.3.1",
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/typescript": "1.3.1",
    "@graphql-codegen/typescript-operations": "^1.3.1",
    "@graphql-codegen/typescript-resolvers": "1.3.1",
    "@types/aws-lambda": "^8.10.17",
    "@types/graphql": "^14.2.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^10.12.18",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.35.2"
  }
}
